{
  "name": "neo-components",
  "version": "9.1.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build ngx-neo-components --prod",
    "build_lib_mat": "ng build ngx-neo-components-mat --prod",
    "build_lib_watch": "ng build ngx-neo-components --watch --prod",
    "build_lib_mat_watch": "ng build ngx-neo-components-mat --watch --prod",
    "npm_pack": "cd dist/ngx-neo-components && npm pack",
    "debug": "npm run build_lib && ng serve --vendorSourceMap",
    "debug_mat": "npm run build_lib_mat && ng serve --vendorSourceMap",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack",
    "copy-license": "cp LICENSE ./dist/ngx-neo-components",
    "copy-readme": "cp README.md ./dist/ngx-neo-components",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "postbuild_lib": "scss-bundle -c scss-bundle.config.json",
    "npm_pack_mat": "cd dist/ngx-neo-components-mat && npm pack",
    "package_mat": "npm run build_lib_mat && npm run copy-files_mat && npm run npm_pack_mat",
    "copy-license_mat": "cp LICENSE ./dist/ngx-neo-components-mat",
    "copy-readme_mat": "cp README.md ./dist/ngx-neo-components-mat",
    "copy-files_mat": "npm run copy-license_mat && npm run copy-readme_mat",
    "postbuild_lib_mat": "scss-bundle -c scss-bundle-mat.config.json",
    "build:cordova": "ng build --configuration=cordova",
    "build:cordova:debug": "npm run build_lib && ng build  --configuration=cordovaDebug",
    "cordova:android": "cd ./cordova && cordova run android --device",
    "cordova:ios": "cd ./cordova && cordova build ios",
    "build:run:android": "npm run build:cordova && npm run cordova:android",
    "build:run:android:debug": "npm run build:cordova:debug && npm run cordova:android",
    "build:run:ios": "npm run build:cordova && npm run cordova:ios",
    "build:run:ios:debug": "npm run build:cordova:debug && npm run cordova:ios"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/localize": "^9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/material-moment-adapter": "~9.1.3",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@neocomplexx/ngx-neo-directives": "^9.0.0",
    "@neocomplexx/ngx-neo-directives-mat": "^9.0.0",
    "@neocomplexx/ngx-neo-modal-mat": "^9.0.0",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "bootstrap": "^4.4.1",
    "hammerjs": "^2.0.8",
    "moment": "^2.27.0",
    "ng-packagr": "^5.4.0",
    "ngx-mask": "^8.2.0",
    "rxjs": "^6.5.5",
    "tsickle": "0.38.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@angular/language-service": "~9.1.9",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.6.0",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^9.0.0",
    "protractor": "^5.4.4",
    "scss-bundle": "^2.5.0",
    "ts-node": "^7.0.1",
    "tslib": "^1.13.0",
    "tslint": "~5.11.0",
    "typescript": "~3.8.3"
  }
}
