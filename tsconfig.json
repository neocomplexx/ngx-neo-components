{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-neo-components": [
        "dist/ngx-neo-components"
      ],
      "ngx-neo-components/*": [
        "dist/ngx-neo-components/*"
      ],
      "ngx-neo-components-mat": [
        "dist/ngx-neo-components-mat"
      ],
      "ngx-neo-components-mat/*": [
        "dist/ngx-neo-components-mat/*"
      ]
    }
  }
}